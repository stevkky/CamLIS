}
.frm-sample-entry .sample-user-info .well {
	border: none;
	padding: 10px 15px;
	margin-bottom: 0;
	background-color: #f9f9f9;
}
#test_list_wrapper {
	margin-top: 20px;
}
.margintop10 {
	margin-top: 10px;
}
#possible_result_modal .modal-dialog {
	margin-top: 50px;
}
#possible_result_modal .modal-content {
	box-shadow: 0 0 8px -5px black !important;
}

#tb_test_result td{
	vertical-align: middle !important;
}

#tb_test_result .header {
	font-weight: bold;
	cursor: pointer;
}
#tb_test_result .header:hover {
	color:#4178db;
}

tr.error_border {
	border-left: 3px solid #ff1212;
}
tr.out-of-range {
	background: orange !important;
	font-weight: bold;
}

#test_modal .department-header {
	background: #2A83D0;
	color:white;
	padding: 8px 10px;
	font-weight: bold;
}
#test_modal .sample-type-wrapper {

}
#test_modal .sample-type-header {
	padding:8px 8px;
	background: #f3ebeb;
	font-weight: bold;
	border-bottom: 2px solid #efdede;
}
#test_modal .btn-toggle-sample-info {
	background: transparent;
	padding-top: 0;
	padding-bottom: 2px;
	border: none;
}
#test_modal .btn-toggle-sample-info:focus {
	outline: none;
}
#test_modal .sample-type-wrapper:not(:first-child) .sample-type-header {
	border-top: 2px solid #f2e1e1;
}
#test_modal .tree-list-wrapper {
	height: 500px;
	box-shadow: 0 5px 4px -4px #6f6e6e;
	margin-bottom: 20px;
	display: flex;
	flex-direction: column;
	padding-bottom: 10px;
	border:2px solid #e7e4e4;
}
#test_modal .tree-list {
	height: 100%;
	width: 100%;
	overflow: auto;
}
#test_modal .tree-list-filter-wrapper {
	padding: 5px;
	border-bottom: 2px solid #e4dede;
}
#test_modal .sample-info-wrapper {
	width: 100%;
	padding : 5px 10px 10px;
	border-top:2px solid #efefef;
	border-bottom:2px solid #e7e4e4;
	background: #f5f5f5;
}
#test_modal .sample-type-header-wrapper + ul {
	margin-top: 10px;
	margin-bottom: 20px;
}

#patient-info-view {
	margin-top: 25px;
	/*border: 2px solid #d7d7d7;
	padding: 12px;*/
}
#patient-info-form {
	background: #faf8f8 !important;
	border-bottom: 2px solid #d7d7d7 !important;
	border-left: 2px solid #d7d7d7 !important;
	border-right: 2px solid #d7d7d7 !important;
}

.checkbox-wrapper {
	display: inline-block;
	padding-right: 20px;
}
.checkbox-wrapper .checkbox-inline {
	padding-left: 0;
}
.print-preview-header {
	height: 50px;
	width: 100%;
	background: rgba(0, 0, 0, 0.7);
	position: fixed;
	top: 0;
	left: 0;
	z-index: 100;
	text-align: center;
}
.print-preview-header ul {
	list-style: none;
	margin: 0;
	padding: 0;
}
.print-preview-header ul li a {
	display: inline-block;
	text-decoration: none;
	color: white;
	padding: 15px 20px;
}
.print-preview-header ul li a:hover {
	background: #2a2a2a;
}
.print-preview-header .close-preview {
	display: block;
	width: 20px;
	height: 20px;
	border-radius: 10px;
	-moz-border-radius: 10px;
	-webkit-border-radius: 10px;
	color: white;
	position: absolute;
	right: 20px;
	top: 15px;
	cursor: pointer;
}
.print-preview-header .close-preview:hover {
	background: white;
	color: black;
}

/* Media Screen */

@media screen and (min-width: 993px) {
	.btn-wrapper .btn {
		width: 100%;
	}
}
@media screen and (max-width: 992px) {
	.btn-wrapper .btn {
		width: 120px;
	}
}

/* Custom Timepicker style*/

.custom_timepicker {
	background: white;
	border-radius: 3px;
}
.custom_timepicker a.reset_time {
	display: inline-block;
	margin-top: 10px;
	text-align: center;
	padding: 5px 0;
	width: 100%;
	text-decoration: none;
	border-radius: 3px;
	-moz-border-radius: 3px;
	-webkit-border-radius: 3px;
}
.custom_timepicker a.reset_time:hover {
	background: #ebebeb;
}
