.table>tbody>tr>td, .table>tbody>tr>th, .table>tfoot>tr>td, .table>tfoot>tr>th, .table>thead>tr>td, .table>thead>tr>th {
	padding: 3px;
	line-height: 1.42857143;
	vertical-align: top;
	border-top: 1px solid #ddd;
}
.modal-body {
	position: relative;
	padding: 35px;
}
.panel {
	border-radius: 0 !important;
}
.panel .panel-heading {
	cursor: pointer;
}
.panel .header {
	display: inline-block;
	font-weight: bold;
}
#sample-forms .input-group > input,
.narrow-padding {
	padding-left: 5px;
	padding-right: 5px;
}
#sample-forms .input-group-addon {
	padding-left: 5px;
	padding-right: 5px;
}
#sample-forms .panel {
	border-top: 3px solid #BECEDB !important;
}
.frm-sample-entry .panel-heading {
	background: #efeded;
	border-radius: 0;
	border-bottom: 1px solid #d9d9d9;
}
.frm-sample-entry .panel-heading .header {
