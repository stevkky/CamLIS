}
.frm-sample-entry .panel-heading .sample-title {
	display: inline-block;
	margin-left: 15px;
}
.frm-sample-entry .panel-heading .sample-title div[class$=title] {
	display: inline-block;
}
.frm-sample-entry .panel-body {
	padding:18px 0 0;
}
.frm-sample-entry .sample-user-info {
	padding: 0;
}
.frm-sample-entry .sample-entry-user-list,
.frm-sample-entry .result-entry-user-list {
